{
  "resourceType": "StructureDefinition",
  "id": "ServicioSolicitante",
  "url": "http://falp.org/StructureDefinition/ServicioSolicitante",
  "name": "ServicioSolicitante",
  "status": "draft",
  "description": "Este es un perfil donde se encontrarán los datos del servicio de salud que realiza la petición de muestra",
  "fhirVersion": "4.0.1",
  "kind": "resource",
  "abstract": false,
  "type": "ServiceRequest",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/ServiceRequest",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "ServiceRequest.identifier",
        "path": "ServiceRequest.identifier",
        "short": "Identificador del servicio solicitante",
        "definition": "Se define el identificador como aquel que lo distingue del resto de los servicios clínicos.",
        "min": 1,
        "max": "1"
      },
      {
        "id": "ServiceRequest.identifier.extension:identificador",
        "path": "ServiceRequest.identifier.extension",
        "sliceName": "identificador",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org/fhir/StructureDefinition/identifier-validDate"
            ]
          }
        ]
      },
      {
        "id": "ServiceRequest.requisition",
        "path": "ServiceRequest.requisition",
        "short": "id de la solicitud compuesta."
      },
      {
        "id": "ServiceRequest.status",
        "path": "ServiceRequest.status",
        "short": "Estado, valores posibles : \tdraft | active | on-hold | revoked | completed | entered-in-error | unknown.",
        "definition": "Estado del diágnostico solicitado del paciente.",
        "patternCode": "active",
        "mustSupport": true
      },
      {
        "id": "ServiceRequest.intent",
        "path": "ServiceRequest.intent",
        "short": "Estado, valores posibles: proposal | plan | directive | order | original-order | reflex-order | filler-order | instance-order | option",
        "definition": "intención",
        "patternCode": "original-order",
        "mustSupport": true
      },
      {
        "id": "ServiceRequest.priority",
        "path": "ServiceRequest.priority",
        "short": "Estado, valores posibles: \troutine | urgent | asap | stat",
        "definition": "intención",
        "patternCode": "urgent",
        "mustSupport": true
      },
      {
        "id": "ServiceRequest.code",
        "path": "ServiceRequest.code",
        "short": "solicitud u orden que se debe llevar a cabo."
      },
      {
        "id": "ServiceRequest.orderDetail",
        "path": "ServiceRequest.orderDetail",
        "short": "Información adicional.",
        "max": "1"
      },
      {
        "id": "ServiceRequest.subject",
        "path": "ServiceRequest.subject",
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://falp.org/StructureDefinition/Paciente"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "ServiceRequest.occurrence[x]",
        "path": "ServiceRequest.occurrence[x]",
        "type": [
          {
            "code": "dateTime"
          }
        ]
      },
      {
        "id": "ServiceRequest.authoredOn",
        "path": "ServiceRequest.authoredOn",
        "definition": "Fecha y hora de firma de la solicitud."
      },
      {
        "id": "ServiceRequest.locationReference",
        "path": "ServiceRequest.locationReference",
        "short": "localización referencial."
      },
      {
        "id": "ServiceRequest.bodySite",
        "path": "ServiceRequest.bodySite",
        "short": "localización de la  muestra en el cuerpo."
      },
      {
        "id": "ServiceRequest.note",
        "path": "ServiceRequest.note",
        "short": "sección de comentarios adicionales."
      },
      {
        "id": "ServiceRequest.patientInstruction",
        "path": "ServiceRequest.patientInstruction",
        "short": "sección de instrucciones al paciente."
      }
    ]
  }
}
